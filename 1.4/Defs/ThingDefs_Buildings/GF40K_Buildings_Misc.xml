<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef ParentName="BuildingBase">
    <defName>GF40K_ImperiumWeaponry</defName>
    <label>Omnissiah's Cogitator Array</label>
    <description>The Omnissiah's Cogitator Array is an intricate and imposing machine, an epitome of technological marvel and arcane knowledge. Constructed by the adept hands of the Adeptus Mechanicus, this massive array of cogitators, data-crunching engines, and forbidden techno-sorceries serves as the pinnacle of research and analysis technology. Encased within a formidable chassis adorned with intricate Mechanicus symbols and glowing energy conduits, the Omnissiah's Cogitator Array hums with unfathomable power and intellect.</description>
    <graphicData>
      <texPath>Things/Building/Misc/ImperiumWeaponry</texPath>
			<shaderType>CutoutComplex</shaderType>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>4</drawSize><!--2.46875-->
    </graphicData>
    <uiIconScale>0.85</uiIconScale>
    <researchPrerequisites><li>GF40K_ImperiumCore</li></researchPrerequisites>
    <size>(2,2)</size>
    <rotatable>false</rotatable>
    <altitudeLayer>Building</altitudeLayer>
    <passability>PassThroughOnly</passability>
    <castEdgeShadows>true</castEdgeShadows>
    <fillPercent>0.5</fillPercent>
    <designationHotKey>Misc4</designationHotKey>
    <designationCategory>Misc</designationCategory>
    <minifiedDef>MinifiedThing</minifiedDef>
    <canOverlapZones>false</canOverlapZones>
    <terrainAffordanceNeeded>Medium</terrainAffordanceNeeded>
    <thingCategories>
      <li>BuildingsMisc</li>
    </thingCategories>
    <statBases>
      <MaxHitPoints>200</MaxHitPoints>
      <WorkToBuild>10000</WorkToBuild>
      <Mass>60</Mass>
      <Flammability>1.0</Flammability>
    </statBases>
    <stuffCategories>
      <li>Metallic</li>
    </stuffCategories>
    <costStuffCount>100</costStuffCount>
    <costList>
      <Plasteel>60</Plasteel>
	  <Uranium>30</Uranium>
      <Gold>10</Gold>
      <ComponentSpacer>6</ComponentSpacer>
    </costList>
    <building>
      <destroySound>BuildingDestroyed_Metal_Medium</destroySound>
    </building>
    <comps>
      <li Class="CompProperties_Power">
        <compClass>CompPowerTrader</compClass>
        <shortCircuitInRain>true</shortCircuitInRain>
        <basePowerConsumption>450</basePowerConsumption>
      </li>
      <li Class="CompProperties_Flickable"/>
      <li Class="CompProperties_Facility">
        <statOffsets>
          <ResearchSpeedFactor>0.15</ResearchSpeedFactor>
        </statOffsets>
        <maxSimultaneous>1</maxSimultaneous>
      </li>
    </comps>
    <placeWorkers>
      <li>PlaceWorker_ShowFacilitiesConnections</li>
    </placeWorkers>
    <constructionSkillPrerequisite>14</constructionSkillPrerequisite>
  </ThingDef>


</Defs>